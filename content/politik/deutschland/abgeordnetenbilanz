      <region cp:type="teaser-bar" module="normal" area="teaser-row-full" cp:__name__="id-02c7d37a-6e3d-4659-8df2-9c35d6c61ac2">
        <container cp:type="teaser" module="ressort" hide-dupes="True" cp:__name__="id-0458dc2b-4538-4988-b407-795318e1ef04" visible="True">
          <block href="http://xml.zeit.de/politik/deutschland/2013-07/abgeordnetenbilanz-mdb-datenprojekt-bundestag" year="2013" issue="28" ressort="Politik" contenttype="article" publication-date="" expires="" date-last-modified="2013-12-02T17:24:16+00:00" date-first-released="2013-07-25T08:29:15.123254+00:00" date-last-published="2013-12-02T18:25:40+00:00" last-semantic-change="2013-07-25T08:21:02.345684+00:00" uniqueId="http://xml.zeit.de/politik/deutschland/2013-07/abgeordnetenbilanz-mdb-datenprojekt-bundestag" author="Lisa Caspari;Paul Blickle;Martina Schories;Sascha Venohr">
            <supertitle py:pytype="str">Deutscher Bundestag</supertitle>
            <title py:pytype="str">Die große Abgeordneten-Bilanz</title>

    <reference type="intern" href="http://xml.zeit.de/2009/22/BRD-220-Jahre" year="2009" issue="22" ressort="Wissen" publication-date="" expires="">
      <supertitle py:pytype="str">Demokratie</supertitle>
      <title py:pytype="str">220 Jahre Bundesrepublik Deutschland</title>

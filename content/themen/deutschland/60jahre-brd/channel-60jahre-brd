    <block href="http://xml.zeit.de/2009/09/Neunerjahre" publication-date="" expires="" year="2009" issue="9" hidden_relateds="true" hp_hide="false" pinned="false">
      <supertitle py:pytype="str">Deutsche Geschichte</supertitle>

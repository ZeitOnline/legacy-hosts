      <block xmlns:py="http://codespeak.net/lxml/objectify/pytype" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" href="http://xml.zeit.de/online/2008/49/cdu-merkel-analyse" publication-date="" expires="" year="2008" issue="49" hp_hide="false" pinned="false" hidden_relateds="true">

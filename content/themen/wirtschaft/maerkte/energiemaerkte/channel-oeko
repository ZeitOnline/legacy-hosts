          <reference type="intern" href="http://xml.zeit.de/2008/16/T-CCS" year="2008" issue="16" date-last-modified="2009-08-18T08:39:59+00:00" last-semantic-change="" date-first-released="2008-04-10T12:00:00+00:00" date-last-published="2008-09-07T18:50:42.959454+00:00" publication-date="" expires="">
            <supertitle py:pytype="str">Kohlekraftwerke</supertitle>
            <title py:pytype="str">Kohle fürs gute Gewissen</title>

      <block xmlns:py="http://codespeak.net/lxml/objectify/pytype" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" href="http://xml.zeit.de/2009/27/U-Solarflugzeug" publication-date="" expires="" year="2009" issue="27" hidden_relateds="true" hp_hide="false" pinned="false">

    <block href="http://xml.zeit.de/politik/2013-11/nsa-untersuchungsausschuss-zeugen-geheimdienste" uniqueId="http://xml.zeit.de/politik/2013-11/nsa-untersuchungsausschuss-zeugen-geheimdienste" year="2013" issue="44" ressort="Politik" contenttype="article" publication-date="" expires="">
      <supertitle py:pytype="str">NSA-Untersuchungsausschuss</supertitle>

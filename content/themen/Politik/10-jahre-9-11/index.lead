    <block href="http://xml.zeit.de/2009/32/L-B-O-Neill" uniqueId="http://xml.zeit.de/2009/32/L-B-O-Neill" year="2009" issue="32" ressort="Kultur" contenttype="article" publication-date="" expires="">

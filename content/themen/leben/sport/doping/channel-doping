    <block href="http://xml.zeit.de/online/2009/31/tour-de-france-armstrong-contador" publication-date="" expires="" year="2009" issue="31" hidden_relateds="false" hp_hide="false">
      <supertitle py:pytype="str">Tour de France</supertitle>

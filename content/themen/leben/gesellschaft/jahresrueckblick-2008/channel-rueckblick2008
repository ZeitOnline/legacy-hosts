    <block href="http://xml.zeit.de/online/2008/52/bg-sicherheit" publication-date="" expires="" year="2008" issue="52" hp_hide="false" pinned="false" hidden_relateds="false">
      <supertitle py:pytype="str">Innere Sicherheit</supertitle>
      <title py:pytype="str">Alles andere als harmlos</title>

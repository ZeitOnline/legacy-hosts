        <container cp:type="fullgraphical" module="fullgraphical" cp:__name__="91415b72-e171-484c-9396-e1d00956328a" title="Quiz - Rätsel auf ZEIT ONLINE">
          <block type="intern" href="http://xml.zeit.de/spiele/quiz/index" year="2012" ressort="Spiele" publication-date="2010-10-07T22:05:00+00:00" expires="" date-last-modified="2012-04-11T15:58:51+00:00" date-first-released="2009-09-06T06:21:59.047928+00:00" date-last-published="2012-04-10T15:05:31.612695+00:00" last-semantic-change="2012-04-10T15:02:56.106041+00:00">

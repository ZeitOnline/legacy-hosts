              <script type="text/javascript" src="/static/js/projekte/tapete-en/alo.js?1503"/>
              <script type="text/javascript">
                jQuery(document).ready(function($) {

                        if( Modernizr.svg &amp;&amp; Modernizr.inlinesvg ) {

                                $('#nonvis-wrapper').css('display', 'none');
                                $('#vis-wrapper').css('display', 'block');

                                if(getUrlVars()["plz"]) {
                                        try { highlightRow(plzToKrs(getUrlVars()["plz"])); } catch(err) {}
                                } else startIntro();

                                $('#main-nav').waypoint(function(event, direction) {
                                        if (direction === 'down') {
                                                $(this).parent().toggleClass('sticky', true);
                                                $('#backtotop').css('display', 'inline')
                                        } else {
                                                $(this).parent().toggleClass('sticky', false);
                                                $('#backtotop').css('display', 'none');
                                        }
                                        guiders.reposition();
                                        event.stopPropagation();
                                });
                                $('#vis-end').waypoint(function(event, direction){
                                        if (direction === 'down') {
                                                $('#main-nav').css('display', 'none');
                                        } else {
                                                $('#main-nav').css('display', 'block');
                                        }
                                        event.stopPropagation();
                                }, { offset: 225 });
                                $('#backtotop').click(function() {
                                        $('html,body').animate( { scrollTop: $("#svg").offset().top - 221}, 500, 'swing');
                                        d3.selectAll(".landBtn").classed("pressed", false);
                                        d3.selectAll(".bg").classed("highlighted", false);
                                });
                                $('#tourBtn').click(function() { startIntro(); });
                        } else {
                                $('#nonvis-wrapper').css('display', 'block');
                        }
                });
        </script>

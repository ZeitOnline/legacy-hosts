<?xml version='1.0' encoding='UTF-8'?>
<settings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <!-- Supported types: xsd:string, xsd:int, xsd:double -->
  <setting name="default_teaser_images" xsi:type="xsd:string">http://xml.zeit.de/zeit-magazin/default/teaser_image</setting>
  <setting name="default_packshot_diezeit" xsi:type="xsd:string">http://xml.zeit.de/administratives/printbanderole/default-packshot-diezeit</setting>
  <setting name="breaking_news_timeout" xsi:type="xsd:int">3600</setting>
  <setting name="author_articles_page_size" xsi:type="xsd:int">10</setting>
  <setting name="author_comment_page_size" xsi:type="xsd:int">10</setting>
  <setting name="author_articles_query" xsi:type="xsd:string">author:"{}" AND (type:article OR type:video OR type:link OR type:gallery)</setting>
  <setting name="comment_page_size" xsi:type="xsd:int">4</setting>
  <!-- Use this setting to determine wheter or not an paragraph should be
       considered for ad places. The number represents the number of
       characters in the paragraph. -->
  <setting name="sufficient_paragraph_length" xsi:type="xsd:int">300</setting>
  <setting name="cp_preserve_modules_on_pagination" xsi:type="xsd:string">headerimage centerpage-header search-form authorlist-alphabet-pager</setting>
  <!-- Timeouts for 3rdparty systems -->
  <setting name="community_host_timeout_secs" xsi:type="xsd:int">3</setting>
  <setting name="brightcove_image_timeout" xsi:type="xsd:int">2</setting>
  <setting name="liveblog_timeout" xsi:type="xsd:int">1</setting>
  <setting name="mailjet_timeout" xsi:type="xsd:int">1</setting>
  <setting name="retresco_timeout" xsi:type="xsd:int">2</setting>
  <setting name="reach_timeout" xsi:type="xsd:double">0.2</setting>
  <setting name="podigee_timeout" xsi:type="xsd:int">1</setting>
  <setting name="podigee_api_timeout" xsi:type="xsd:int">1</setting>
  <setting name="podigee_config_timeout" xsi:type="xsd:int">1</setting>
  <setting name="script_timeout" xsi:type="xsd:int">2000</setting>
  <setting name="solr_timeout" xsi:type="xsd:int">2</setting>
  <setting name="spektrum_hp_feed_timeout" xsi:type="xsd:int">2</setting>
  <setting name="zett_hp_feed_timeout" xsi:type="xsd:int">2</setting>
  <!-- Cache times for browsers -->
  <!-- Most content types should have 0, so that browsers re-request them after login/logout, see BUG-343 -->
  <setting name="caching_time_content" xsi:type="xsd:int">0</setting>
  <setting name="caching_time_article" xsi:type="xsd:int">0</setting>
  <setting name="caching_time_centerpage" xsi:type="xsd:int">0</setting>
  <setting name="caching_time_feed" xsi:type="xsd:int">0</setting>
  <setting name="caching_time_gallery" xsi:type="xsd:int">0</setting>
  <setting name="caching_time_video" xsi:type="xsd:int">0</setting>
  <!-- XXX mostly for /konto -->
  <setting name="caching_time_rawxml" xsi:type="xsd:int">0</setting>
  <!-- 90 * 24 * 60 * 60 -->
  <setting name="caching_time_image" xsi:type="xsd:int">7776000</setting>
  <setting name="caching_time_videostill" xsi:type="xsd:int">600</setting>
  <setting name="caching_time_external" xsi:type="xsd:int">600</setting>
  <setting name="caching_time_text" xsi:type="xsd:int">600</setting>
  <setting name="caching_time_file" xsi:type="xsd:int">600</setting>
  <setting name="caching_time_unknown" xsi:type="xsd:int">600</setting>
  <!-- Cache times for Varnish -->
  <setting name="varnish_caching_time_content" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_article" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_centerpage" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_feed" xsi:type="xsd:int">600</setting>
  <!-- Google wants sitemaps to be as current as possible -->
  <setting name="varnish_caching_time_sitemap" xsi:type="xsd:int">3</setting>
  <setting name="varnish_caching_time_gallery" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_video" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_image" xsi:type="xsd:int">3600</setting>
  <setting name="varnish_caching_time_videostill" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_external" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_text" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_rawxml" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_file" xsi:type="xsd:int">600</setting>
  <setting name="varnish_caching_time_unknown" xsi:type="xsd:int">600</setting>
  <setting name="volume_navigation_base" xsi:type="xsd:string">https://premium.zeit.de/abo/diezeit/%s/%02d</setting>
  <!-- overscrolling settings -->
  <setting name="overscrolling_active" xsi:type="xsd:boolean">true</setting>
  <!-- homepage hp overlay settings -->
  <setting name="hp_overlay_active" xsi:type="xsd:boolean">true</setting>
  <!-- homepage hp overlay interval, in seconds -->
  <setting name="hp_overlay_interval" xsi:type="xsd:int">78</setting>
  <!-- updateSignals settings, reminder on/off is now a feature toggle -->
  <!-- homepage update signals interval, in seconds -->
  <setting name="update_signals_comments_interval" xsi:type="xsd:int">20</setting>
  <setting name="update_signals_time_interval" xsi:type="xsd:int">60</setting>
  <!-- URL prefixes -->
  <setting name="asset_prefix" xsi:type="xsd:string">/static/{version}</setting>
  <setting name="image_prefix" xsi:type="xsd:string">https://img.zeit.de</setting>
  <setting name="jsconf_prefix" xsi:type="xsd:string">http://www.zeit.de/static/js</setting>
  <setting name="fbia_prefix" xsi:type="xsd:string">http://fbia.zeit.de/fbia</setting>
  <setting name="c1_prefix" xsi:type="xsd:string">http://www.zeit.de/cre-1.0</setting>
  <setting name="webtrekk_version" xsi:type="xsd:string">4.4.1</setting>
  <setting name="webtrekk_id" xsi:type="xsd:string">981949533494636</setting>
  <setting name="series_prefix" xsi:type="xsd:string">http://xml.zeit.de/serie</setting>
  <setting name="topic_prefix" xsi:type="xsd:string">http://xml.zeit.de/thema</setting>
  <setting name="newsletter_content_host" xsi:type="xsd:string">http://www.zeit.de/</setting>
  <setting name="newsletter_image_host" xsi:type="xsd:string">https://img.zeit.de/</setting>
  <!-- Do not use in other contexts than framebuilders useSSL variant, has no local testing alternative -->
  <setting name="framebuilder_ssl_asset_prefix" xsi:type="xsd:string">https://ssl.zeit.de/www.zeit.de/static/latest</setting>
  <setting name="paywall_premium_product_digitalonly" xsi:type="xsd:string">https://premium.zeit.de/bestellung/1579642</setting>
  <setting name="paywall_premium_product_digitalandprint" xsi:type="xsd:string">https://premium.zeit.de/bestellung/1596805</setting>
  <setting name="paywall_premium_redirect" xsi:type="xsd:string">https://premium.zeit.de/abo/paywall</setting>
  <!-- podcast player -->
  <setting name="podcast_theme_cp" xsi:type="xsd:string">zon-standalone</setting>
  <setting name="podcast_theme_article_body" xsi:type="xsd:string">zon-standalone</setting>
  <setting name="podcast_theme_article_header" xsi:type="xsd:string">zon-minimal</setting>
  <!-- To enable, insert path to optimizely Script (//cdn.optimizely.com/js/281825380.js). To disable, use empty string. Do NOT use "false". -->
  <setting name="optimizely_on_zon_homepage" xsi:type="xsd:string"/>
  <setting name="optimizely_on_zon_centerpage" xsi:type="xsd:string"/>
  <setting name="optimizely_on_zon_article" xsi:type="xsd:string"/>
  <!-- Facebook Page IDs fb:pages meta tag -->
  <setting name="facebook_app_id_list" xsi:type="xsd:string">37816894428, 63948163305, 327602816926, 114803848589834</setting>
  <!-- must be configured as an accepted sender at https://app.mailjet.com/account/sender -->
  <setting name="mailjet_sender" type="xsd:string">online-entwicklungsredaktion@zeit.de</setting>
</settings>
